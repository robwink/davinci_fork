			struc.xaxis = xaxis
		}
	
	} else {
		# user did specify an 'inst' -  test for valid values of inst
		if(inst == "lab1" || inst == "lab2" || inst == "lab3" || inst == "tes" || inst == "tes5" || inst == "tes73" || inst == "mtes" || inst == "otesm" || inst == "otesv" || inst == "otes" || strstr(inst,"otes")==1 || inst == "themis" || inst == "aster" || inst =="tims") {
			# make the xaxis
			xaxis = make_band(inst= inst)	
			struc.xaxis = xaxis
			nbands = dim(xaxis)[3]
	
		} else {
			printf("not a valid instrument\n")
			return(0)
		}
	}
	struc.inst = inst

	# do 'sanity checking' on x, y, and z dimensions
	size_spec = dim(spec)
	size_irf = dim(irf)
	size_ri = dim(ri)
	size_xaxis = dim(xaxis)

	# irf and ri must be same size
	if((size_irf[1] != size_ri[1]) || (size_irf[2] != size_ri[2]) || (size_irf[3] != size_ri[e])) {
		printf("size of irf and ri not equal\n")
		return(0)
	}

	# z length of irf/ri must match xaxis
	if(size_xaxis[3] != size_irf[3]) {
		printf("z dimenstion of xaxis and irf/ri don't match\n")
		return(0)
	}
	
	if(size_xaxis[3] != size_irf[3]) {
		printf("length of xaxis for this instrument and length of irf/ri don't match\n")
		return(0)
	}
	
	if((size_irf[1] == 1) && (size_irf[2] == 1) && (size_ri[1] == 1) && (size_ri[2] == 1)) {
	   # x and y dimension of irf and ri equal 1 - guarenteed to be ok

	} else if((size_irf[1] == size_spec[1]) && (size_irf[2] == size_spec[2])) {
	  # if x dimension of irf/ri and spec the same - ok

	} else {
	  # not ok
