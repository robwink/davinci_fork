MODSUPPLIB=modsupp.lib
LIBS   = hdf5.lib $(MODSUPPLIB) iomedley.lib 

LIBIOMEDLEYDIR = iomedley
LIBIOMEDLEY = $(LIBIOMEDLEYDIR)/libiomedley.a


.SUFFIXES: .obj .lib

.c.obj:
	$(CC) -c $(CPPFLAGS) $(DEFS) $(CFLAGS) $< -Fo$@

