x.obj:    x.c
	$(CC) -c $(CPPFLAGS) $(CFLAGS) $(DEFS) $(XRTINCLUDE) $*.c -Fo$@

xrt_print_3d.obj: xrt_print_3d.c
	$(CC) -c $(CPPFLAGS) $(CFLAGS) $(DEFS) $(XRTINCLUDE) $*.c -Fo$@

config.h:	configwin.h
	copy configwin.h config.h


