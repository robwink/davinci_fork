CC=cl
CPPFLAGS=-I. -Ilib -Iwin32 -Iiomedley
CFLAGS=-Yd -nologo -ZI -GZ
LDFLAGS=-libpath:win32 -libpath:iomedley

#DEFS   = -DNEED_UDEFS -D_CONSOLE -DHAVE_STRTOUL -DBUILD_MODULE_SUPPORT -D_WIN32 -DMSDOS -D_MSDOS -D_MSDOS_ -DINCLUDE_API
